<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SonRise.StageDevelopmentMain"
             Title="Development"
             BackgroundColor="LightSkyBlue">
    <ContentPage.Content>
        <StackLayout Padding="20,20,20,20">
            <Button BackgroundColor="White" WidthRequest="100" HeightRequest="100" Text="Current Development"/>

            <Button BackgroundColor="White" WidthRequest="100" HeightRequest="100" Text="New Evaluation" Clicked="Button_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>